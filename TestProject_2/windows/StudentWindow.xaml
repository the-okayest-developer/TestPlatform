<Window x:Class="TestProject_2.windows.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestProject_2.windows"
        mc:Ignorable="d"
        Title="StudentWindow" Height="530" Width="600"
        Closing="onWindow_Closing"
        Loaded="onWindow_Loaded">
    <Grid>
        <Label Content="Good luck!" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="27" Width="213"/>
        <Label Content="Test Name:" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" RenderTransformOrigin="2.096,1.603" Height="29" Width="71"/>
        <TextBlock x:Name="testNameTextBox" HorizontalAlignment="Left" Margin="86,47,0,0" TextWrapping="Wrap" Text="TestName" VerticalAlignment="Top" Width="145" Height="19"/>
        <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="10,70,10,60">
            <StackPanel x:Name="testQuestionsPanel" Margin="10,10,10,10" HorizontalAlignment="Left"/>
        </ScrollViewer>
        <Button Content="Submit Answers" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20" Padding="10,5,10,5" Click="submitAnswersButton_Click"/>
    </Grid>
</Window>
